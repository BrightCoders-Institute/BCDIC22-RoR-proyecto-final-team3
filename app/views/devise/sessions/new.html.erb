<div class="two-container">
  <div class="image-container3">
      <%=image_tag("svg/foodpoint.svg")%>  
  </div>
  <div class="register-container">
    <div class="register"> 
      <%=image_tag("icon3.png")%>  
      <div class="title-container">
        <h1>Log in!</h1>
      </div>
      <div class="form-container">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="info-container">
        <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "User", class: "user_field" %>
      </div>
      <div class="info-container">
        <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class: "password_field" %>
      </div>
      <div class="button-container">
        <%= f.submit "Log in" %>
      </div>
    <% end %>
      </div>
    </div>
  </div>
</div>

